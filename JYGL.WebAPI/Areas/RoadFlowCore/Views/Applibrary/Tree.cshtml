<form id="form1">
    <div id="tree"></div>
</form>
<script type="text/javascript">
    var roadTree = null;
    $(function ()
    {
        roadTree = new RoadUI.Tree({ id: "tree", path: "../Dictionary/Tree1?ischild=1&root=@ViewData["rootId"]", refreshpath: "../Dictionary/TreeRefresh", onclick: openUrl });
    });
    function openUrl(json)
    {
        parent.frames[1].location = "List?typeid=" + json.id + "&@Html.Raw(ViewData["query"])";
    }
    function reLoad(id)
    {
        roadTree.refresh(id);
    }
</script>
