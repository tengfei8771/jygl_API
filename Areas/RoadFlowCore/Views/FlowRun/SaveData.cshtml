<script type="text/javascript">
    var instanceId = "@ViewData["instanceId"]";
    var errMsg = "@ViewData["errMsg"]";
    var opation = "@ViewData["opation"]";
    RoadUI.Core.closeWait();
    new RoadUI.Window().close();
    if (errMsg) {
        alert(errMsg);
    }
    else {
        $('#form_instanceid',parent.document).val(instanceId);
        parent.@(ViewData["opation"])(true);
    }
</script>
